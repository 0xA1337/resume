{{#resume.basics}}
  <div class="header">
    <h1>{{name}} <span class="header-label">â€“ {{label}}</span></h1>
  </div>
  

  {{#if location}}
    <p class="location grey">
      {{location.city}}, {{location.countryCode}}

      {{#if email }}
        | {{ email }}
      {{/if}}

      {{#if website }}
        | <a href='{{website}}' target='_blank' rel='noopener'>{{website}}</a>
      {{/if}}
