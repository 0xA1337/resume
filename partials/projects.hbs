{{#if resume.publications.length}}
  {{#each resume.publications}}
    <section class='position'>
      {{#if @first}}
        <h2>Projects</h2>
      {{/if}}
      <h3>
        {{#if website}}
          <a target='_blank' rel='noopener' href='{{website}}'>{{name}}</a>
        {{else}}
          {{name}}
        {{/if}}
      </h3>

      <h4>
        {{publisher}}
        {{#if releaseDate}}
          |
          {{date releaseDate 'MMM YYYY'}}
        {{/if}}
      </h4>

      {{#if summary}}
        <p>{{summary}}</p>
      {{/if}}
    </section>
  {{/each}}
{{/if}}